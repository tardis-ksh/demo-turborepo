{
  "$schema": "https://turbo.build/schema.v1.json",
  "globalDependencies": ["**/.env.*local", "tsconfig.json"],
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build1": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build1"]
    },
    "typeCheck": {
      "dependsOn": ["topo"]
    }
  }
}
